<?xml version="1.0" encoding="UTF-8"?>
<Application
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="http://rtf.iter.org/1.0" xsi:schemaLocation="http://rtf.iter.org/1.0 rtf.xsd" Name="app01">
    <Description/>
    <FunctionBlock Name="reader1" Type="ConsoleReader<float64>">
        <OutputPort Name="Out" Signal="a" />
    </FunctionBlock>

    <FunctionBlock Name="sam" Type="GenSample<float64>">
            <InputPort Name="In" Signal="a"/>
            <OutputPort Name="Out" Signal="c"/>
    </FunctionBlock>
    <FunctionBlock Name="sim" Type="SimpleWrite<float64>">
        <InputPort Name="In" Signal="a" />
        <OutputPort Name="Out" Signal="b" />
    </FunctionBlock>

    <FunctionBlock Name="writer" Type="ConsoleWriter<float64>">
        <InputPort Name="In" Signal="b" />
    </FunctionBlock>
    <FunctionBlock Name="writer1" Type="ConsoleWriter<Sample<float64>>">
        <InputPort Name="In" Signal="c" />
    </FunctionBlock>
</Application>
